generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String   @id @default(uuid()) @db.Uuid
  name      String
  email     String   @unique
  cellphone String   @unique
  password  String
  birthDay  String
  createdAt DateTime @default(now())
  updatedAt DateTime @default(now())

  @@map("users")
}
